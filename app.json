{
  "expo": {
    "name": "HelpBee",
    "slug": "HelpBee",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "../src/Images/logo_no_background.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "../src/Images/logo_no_background.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "../src/Images/logo_no_background.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "../src/Images/logo_no_background.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "../src/Images/logo.jpg",
          "color": "#ffcb05"
        }
      ]
    ]
  }
}
